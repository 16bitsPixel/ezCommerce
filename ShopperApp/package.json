    "@types/react-dom": "*",
    "eslint": "*",
    "eslint-config-next": "*",
    "eslint-config-typescript": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.17",
    "@mui/material": "^5.15.17",
    "class-validator": "*",
    "graphql": "*",
    "graphql-request": "*",
    "graphql-yoga": "*",
    "i18next": "^22.4.10",
    "next": "*",
    "next-i18next": "^13.1.5",
    "react": "*",
    "react-dom": "*",
    "reflect-metadata": "*",
    "type-graphql": "*"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "typescript"
    ],
    "parserOptions": {
      "ecmaVersion": 13
